  #include <stdio.h>
  
  int AddToTop(int top)
  {
      printf("Enter AddToTop\n");
  
      int count = 0;
      for(int i = 1;i <= top; ++i)
      {
         count += i;
      }
  
      printf("Quit AddToTop\n");                                                                         
      return count;
  }
  
  int main(void)
  {
      int top = 10;
      int ret = AddToTop(top);
  
      printf("ret = %d\n", ret);
      return 0;
  }
